{
  "name": "@effective/eslint-config",
  "version": "0.1.0",
  "description": "Pre-generated, permutation-based ESLint flat configs for modern TypeScript & React projects",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./modules": {
      "types": "./dist/modules.d.ts",
      "import": "./dist/modules.js"
    }
  },
  "files": [
    "dist/"
  ],
  "scripts": {
    "build": "tsup",
    "generate": "node --import tsx src/build/generate.ts",
    "prepublishOnly": "npm run build && npm run generate",
    "test": "vitest run"
  },
  "keywords": [
    "eslint",
    "eslintconfig",
    "typescript",
    "react",
    "flat-config",
    "oxlint"
  ],
  "license": "MIT",
  "peerDependencies": {
    "eslint": ">= 9.22",
    "typescript": ">= 5.0"
  },
  "dependencies": {
    "@cspell/eslint-plugin": "^8.17.0",
    "@eslint/js": "^9.22.0",
    "@eslint/json": "^1.0.0",
    "@eslint/markdown": "^7.5.0",
    "@vitest/eslint-plugin": "^1.6.0",
    "eslint-config-prettier": "^10.1.0",
    "eslint-plugin-import-x": "^4.6.0",
    "eslint-plugin-jsdoc": "^62.7.0",
    "eslint-plugin-jsx-a11y": "^6.10.0",
    "eslint-plugin-mdx": "^3.2.0",
    "eslint-plugin-n": "^17.15.0",
    "eslint-plugin-oxlint": "^0.15.0",
    "eslint-plugin-perfectionist": "^4.7.0",
    "eslint-plugin-playwright": "^2.5.0",
    "eslint-plugin-react": "^7.37.0",
    "eslint-plugin-react-compiler": "19.1.0-rc.2",
    "eslint-plugin-react-hooks": "^7.0.0",
    "eslint-plugin-regexp": "^2.7.0",
    "eslint-plugin-security": "^3.0.0",
    "eslint-plugin-simple-import-sort": "^12.1.0",
    "eslint-plugin-sonarjs": "^3.0.0",
    "eslint-plugin-storybook": "^0.11.0",
    "eslint-plugin-testing-library": "^7.1.0",
    "eslint-plugin-unicorn": "^58.0.0",
    "eslint-plugin-unused-imports": "^4.1.0",
    "globals": "^17.0.0",
    "typescript-eslint": "^8.20.0"
  },
  "devDependencies": {
    "@oxlint/migrate": "^1.50.0",
    "@types/node": "^25.3.2",
    "eslint": "^9.22.0",
    "oxlint": "^1.50.0",
    "tsup": "^8.3.0",
    "tsx": "^4.19.0",
    "typescript": "^5.7.0",
    "vitest": "^3.0.0"
  }
}
