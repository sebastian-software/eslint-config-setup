{
  "name": "eslint-config-setup",
  "version": "1.0.0",
  "private": false,
  "description": "Pre-generated, opinionated ESLint configurations for TypeScript projects. Zero runtime overhead. Full type-checking. AI-ready.",
  "keywords": [
    "eslint",
    "eslint-config",
    "eslint-flat-config",
    "typescript",
    "react",
    "vitest",
    "playwright",
    "storybook",
    "ai",
    "linting",
    "type-checking"
  ],
  "license": "Apache-2.0",
  "author": {
    "name": "Sebastian Werner",
    "email": "s.werner@sebastian-software.de",
    "url": "http://sebastian-software.de/werner"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sebastian-software/eslint-config-setup.git"
  },
  "homepage": "https://github.com/sebastian-software/eslint-config-setup#readme",
  "bugs": {
    "url": "https://github.com/sebastian-software/eslint-config-setup/issues"
  },
  "funding": {
    "type": "github",
    "url": "https://github.com/sponsors/sebastian-software"
  },
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rm -rf dist && tsx src/build.ts && tsc -p tsconfig.build.json",
    "check": "tsc --noEmit",
    "fix": "eslint --fix .; prettier --write .",
    "format": "prettier --write .",
    "lint": "eslint .",
    "prepublishOnly": "npm run build",
    "test": "vitest",
    "update": "ncu -ui"
  },
  "dependencies": {
    "eslint-plugin-jsdoc": "^62.7.1",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-n": "^17.24.0",
    "eslint-plugin-playwright": "^2.7.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-compiler": "19.1.0-rc.2",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-regexp": "^3.0.0",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-sonarjs": "^4.0.0",
    "eslint-plugin-storybook": "^10.2.12",
    "eslint-plugin-testing-library": "^7.16.0",
    "typescript-eslint": "^8.56.1"
  },
  "devDependencies": {
    "@eslint/js": "^10.0.1",
    "@types/node": "^25.3.0",
    "@vitest/coverage-v8": "^4.0.18",
    "@vitest/eslint-plugin": "^1.6.9",
    "eslint": "^10.0.2",
    "eslint-config-prettier": "^10.1.8",
    "jiti": "^2.6.1",
    "npm-check-updates": "^19.5.0",
    "prettier": "^3.8.1",
    "prettier-plugin-packagejson": "^3.0.0",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "vitest": "^4.0.18"
  },
  "packageManager": "pnpm@10.28.2",
  "engines": {
    "node": ">=22.0.0"
  },
  "publishConfig": {},
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild"
    ]
  }
}
